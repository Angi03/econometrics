о\n","    # до двух знаков после запятой\n","    print(round(z, 2))\n","elif z<=70 and z>50:\n","    print(\"Satisfactory\")\n","    print(round(z, 2))\n","# Во всех остальных случаях,\n","# когда число меньше или равно 50\n","else:\n","  print(\"Not satisfactory\")\n","  print(round(z, 2))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"9Vo0VRpPEzR7","executionInfo":{"status":"ok","timestamp":1713954382975,"user_tz":-180,"elapsed":2,"user":{"displayName":"Схведиани Анги","userId":"01137790840553189688"}},"outputId":"b7cd97b2-79fe-4820-a6c2-09fb633bbe6b"},"execution_count":59,"outputs":[{"output_type":"stream","name":"stdout","text":["Not satisfactory\n","42.64\n"]}]},{"cell_type":"markdown","source":["#FOR, WHILE, CONTINUE"],"metadata":{"id":"NHC8H8eILEpu"}},{"cell_type":"code","source":["# Создание списка друзей\n","friends = ['Joseph', 'Glenn', 'Sally']\n","# Цикл для каждого друга в списке друзей\n","for friend in friends:\n","    # Вывод имени друга\n","    print(friend)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"G1wlkmmVLDhG","executionInfo":{"status":"ok","timestamp":1713954493060,"user_tz":-180,"elapsed":2,"user":{"displayName":"Схведиани Анги","userId":"01137790840553189688"}},"outputId":"2b2fea31-6164-4a49-acab-342a7779f57e"},"execution_count":61,"outputs":[{"output_type":"stream","name":"stdout","text":["Joseph\n","Glenn\n","Sally\n"]}]},{"cell_type":"code","source":["# Цикл для каждого друга в списке друзей\n","for friend in friends:\n","    # Вывод сообщения \"Happy New Year:\" и имени друга\n","    print('Happy New Year:', friend)\n","    # Вывод сообщения \"Done!\" после каждого друга\n","    print('Done!')"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"nd2W2m88JGNx","executionInfo":{"status":"ok","timestamp":1713954543213,"user_tz":-180,"elapsed":3,"user":{"displayName":"Схведиани Анги","userId":"01137790840553189688"}},"outputId":"427bdb30-ae5b-4e2d-beb1-a7f45637d30e"},"execution_count":